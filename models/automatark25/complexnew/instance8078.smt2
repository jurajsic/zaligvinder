(declare-const X String)
; ^[SFTG]\d{7}[A-Z]$
(assert (not (str.in_re X (re.++ (re.union (str.to_re "S") (str.to_re "F") (str.to_re "T") (str.to_re "G")) ((_ re.loop 7 7) (re.range "0" "9")) (re.range "A" "Z") (str.to_re "\u{a}")))))
; ^[a-zA-Z]{4}\d{6}[a-zA-Z]{6}\d{2}$
(assert (not (str.in_re X (re.++ ((_ re.loop 4 4) (re.union (re.range "a" "z") (re.range "A" "Z"))) ((_ re.loop 6 6) (re.range "0" "9")) ((_ re.loop 6 6) (re.union (re.range "a" "z") (re.range "A" "Z"))) ((_ re.loop 2 2) (re.range "0" "9")) (str.to_re "\u{a}")))))
; [^A-Za-z0-9 ]
(assert (not (str.in_re X (re.++ (re.union (re.range "A" "Z") (re.range "a" "z") (re.range "0" "9") (str.to_re " ")) (str.to_re "\u{a}")))))
; /\.js\/\?[a-z]+\=[a-z]{1,4}/R
(assert (not (str.in_re X (re.++ (str.to_re "/.js/?") (re.+ (re.range "a" "z")) (str.to_re "=") ((_ re.loop 1 4) (re.range "a" "z")) (str.to_re "/R\u{a}")))))
; hg diff --nodates | egrep -e "---" -v | egrep -e "^-" -c
(assert (not (str.in_re X (re.union (str.to_re "hg diff --nodates ") (str.to_re " egrep -e \u{22}---\u{22} -v ") (str.to_re " egrep -e \u{22}-\u{22} -c\u{a}")))))
(check-sat)
