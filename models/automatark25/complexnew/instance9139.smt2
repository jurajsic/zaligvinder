(declare-const X String)
; ^(97(8|9))?\d{9}(\d|X)$
(assert (not (str.in_re X (re.++ (re.opt (re.++ (str.to_re "97") (re.union (str.to_re "8") (str.to_re "9")))) ((_ re.loop 9 9) (re.range "0" "9")) (re.union (re.range "0" "9") (str.to_re "X")) (str.to_re "\u{a}")))))
; ^[a-zA-Z]{1,3}\[([0-9]{1,3})\]
(assert (not (str.in_re X (re.++ ((_ re.loop 1 3) (re.union (re.range "a" "z") (re.range "A" "Z"))) (str.to_re "[") ((_ re.loop 1 3) (re.range "0" "9")) (str.to_re "]\u{a}")))))
(check-sat)
