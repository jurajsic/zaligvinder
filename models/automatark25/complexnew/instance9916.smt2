(set-logic QF_SLIA)
(declare-const X String)
; /filename=[^\n]*\u{2e}jpx/i
(assert (str.in_re X (re.++ (str.to_re "/filename=") (re.* (re.comp (str.to_re "\u{a}"))) (str.to_re ".jpx/i\u{a}"))))
; (\d{4,6})
(assert (str.in_re X (re.++ ((_ re.loop 4 6) (re.range "0" "9")) (str.to_re "\u{a}"))))
(check-sat)
