(set-logic QF_SLIA)
(declare-const X String)
; www\u{2E}purityscan\u{2E}com.*
(assert (not (str.in_re X (re.++ (str.to_re "www.purityscan.com") (re.* re.allchar) (str.to_re "\u{a}")))))
; \b4[0-9]\b
(assert (not (str.in_re X (re.++ (str.to_re "4") (re.range "0" "9") (str.to_re "\u{a}")))))
(check-sat)
