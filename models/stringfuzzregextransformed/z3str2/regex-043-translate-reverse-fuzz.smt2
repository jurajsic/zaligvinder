(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.union (str.to_re "U") (str.to_re "~")))))
(assert (= 0 (str.to_int x)))
(assert (not (= x "_U")))
(assert (not (= x "uX")))
(assert (not (= x "s]`")))
(assert (not (= x "___")))
(assert (not (= x "Ut")))
(assert (not (= x "_f2e")))
(assert (not (= x "n<fCd'\t'")))
(check-sat)
