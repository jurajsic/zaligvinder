(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.union (str.to_re "a") (re.+ (str.to_re "w")))))
(assert (str.in_re x (re.union (str.to_re "c") (re.+ (str.to_re "~")))))
(assert (str.in_re x (re.++ (str.to_re "0") (re.union (re.+ (str.to_re "^")) (re.* (str.to_re ""))))))
(check-sat)
