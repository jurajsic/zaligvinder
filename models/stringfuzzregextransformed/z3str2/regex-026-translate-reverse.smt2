(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (str.to_re "'b0x\\'") (re.* (str.to_re "c"))))))
(assert (str.in_re y (re.* (re.++ (str.to_re "'b0x\\'") (re.* (str.to_re "c"))))))
(assert (not (= x y)))
(assert (= (str.len x) (str.len y)))
(check-sat)
