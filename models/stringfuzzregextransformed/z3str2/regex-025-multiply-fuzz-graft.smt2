(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.++ (re.* (str.to_re "P")) (str.to_re "UTb")))))
(assert (str.in_re y (re.+ (re.union (str.to_re "") (re.+ (str.to_re "aa"))))))
(assert (= (str.len x) (str.to_int y)))
(assert (not (= x y)))
(assert (= 6 (str.len x)))
(check-sat)
