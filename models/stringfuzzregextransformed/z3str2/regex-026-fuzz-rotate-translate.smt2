(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.union (re.* (str.to_re "M")) (str.to_re "X")))))
(assert (str.in_re y (re.* (re.union (re.+ (str.to_re "M")) (str.to_re "X")))))
(assert (not (= x y)))
(assert (= (str.len x) (str.to_int y)))
(check-sat)
