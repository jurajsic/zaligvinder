(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.union (str.to_re "'\u{c}'") (str.to_re "`")))))
(assert (= 3 (str.len x)))
(assert (not (= x "``'\u{c}'")))
(assert (not (= x "'\u{c}'`'\u{c}'")))
(assert (not (= x "'\u{c}'``")))
(assert (not (= x "```")))
(assert (not (= x "'\u{c}''\u{c}'`")))
(assert (not (= x "`'\u{c}'`")))
(assert (not (= x "'\u{c}''\u{c}''\u{c}'")))
(check-sat)
