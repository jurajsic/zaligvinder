(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.++ (str.to_re "") (re.+ (str.to_re "Z")))))
(assert (str.in_re x (re.++ (str.to_re "") (re.union (re.* (str.to_re "b")) (re.+ (str.to_re "}"))))))
(assert (= 1 (str.to_int x)))
(check-sat)
