(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.range "0" "9"))))
(assert (= (str.to_int x) 0))
(assert (not (= x "1")))
(assert (not (= x "")))
(assert (not (= x "3")))
(assert (not (= x "%")))
(assert (not (= x "(")))
(assert (not (= x "V")))
(assert (not (= x "4")))
(assert (not (= x "T")))
(assert (not (= x "7")))
(check-sat)
