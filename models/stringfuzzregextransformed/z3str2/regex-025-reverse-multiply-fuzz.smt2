(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (str.to_re "bw/") (re.* (str.to_re "amm"))))))
(assert (str.in_re y (re.* (re.union (str.to_re "j") (re.+ (str.to_re "axm"))))))
(assert (= (str.len x) (str.to_int y)))
(assert (not (= x y)))
(assert (= (str.to_int x) 28))
(check-sat)
