(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.union (re.* (str.to_re "f")) (str.to_re "d")))))
(assert (str.in_re y (re.+ (str.to_re "X"))))
(assert (= 1 (str.to_int y)))
(assert (not (= x y)))
(assert (= (str.to_int x) (str.to_int x)))
(check-sat)
