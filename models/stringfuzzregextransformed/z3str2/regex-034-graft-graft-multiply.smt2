(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (= (str.++ x y) (str.++ m n)))
(assert (str.in_re n (str.to_re "aabbcc")))
(assert (> 2 (str.len m)))
(assert (= (str.len y) (str.len n)))
(assert (= (str.len x) 6))
(check-sat)
