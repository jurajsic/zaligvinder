(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.union (str.to_re "a") (re.+ (str.to_re ""))))))
(assert (str.in_re y (re.+ (re.++ (str.to_re "|") (re.+ (str.to_re "G"))))))
(assert (not (= x y)))
(assert (= (str.to_int x) (str.to_int y)))
(check-sat)
