(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (= (str.++ y x) (str.++ n m)))
(assert (str.in_re n (re.+ (str.to_re "c'\n'<l\\'\u{c}'a3C\\bI>"))))
(assert (> (str.len x) (str.to_int m)))
(assert (= 10 (str.to_int n)))
(assert (= 0 (str.len y)))
(check-sat)
