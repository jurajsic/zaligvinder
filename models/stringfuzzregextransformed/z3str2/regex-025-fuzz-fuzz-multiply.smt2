(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.union (re.* (str.to_re "xx")) (str.to_re "bb")))))
(assert (str.in_re y (re.+ (re.++ (re.+ (str.to_re "aa")) (str.to_re "{{")))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 6))
(check-sat)
