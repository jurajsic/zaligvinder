(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.+ (str.to_re "b")) (str.to_re "")))))
(assert (str.in_re y (re.* (re.union (str.to_re "a") (re.* (str.to_re "X"))))))
(assert (= 11 (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) (str.len x)))
(check-sat)
