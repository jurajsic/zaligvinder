(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.union (str.to_re "&") (re.+ (str.to_re ""))))))
(assert (str.in_re y (re.+ (re.++ (str.to_re "") (re.* (str.to_re "b"))))))
(assert (= (str.to_int x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 14))
(check-sat)
