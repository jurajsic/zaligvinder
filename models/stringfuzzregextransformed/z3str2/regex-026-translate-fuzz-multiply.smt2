(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.++ (re.+ (str.to_re "ZZ")) (str.to_re "!!")))))
(assert (str.in_re y (re.+ (re.++ (re.+ (str.to_re "GG")) (str.to_re "||")))))
(assert (not (= x y)))
(assert (= (str.len x) (str.to_int y)))
(check-sat)
