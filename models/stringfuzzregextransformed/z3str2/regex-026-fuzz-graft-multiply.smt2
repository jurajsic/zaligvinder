(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.union (re.* (re.+ (str.to_re "aa"))) (str.to_re "bb")))))
(assert (str.in_re y (re.* (re.union (str.to_re "aa") (str.to_re "bb")))))
(assert (not (= x y)))
(assert (= (str.len x) (str.to_int y)))
(check-sat)
