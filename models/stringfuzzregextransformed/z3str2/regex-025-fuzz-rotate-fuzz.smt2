(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.union (re.* (str.to_re "")) (str.to_re "w")))))
(assert (str.in_re y (re.* (re.union (re.+ (str.to_re "b")) (str.to_re "a")))))
(assert (= (str.len x) (str.to_int y)))
(assert (not (= x y)))
(assert (= (str.to_int x) 3))
(check-sat)
