(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.range "0" "9"))))
(assert (= (str.to_int x) 2))
(assert (not (= x "11")))
(assert (not (= x "00")))
(assert (not (= x "33")))
(assert (not (= x "22")))
(assert (not (= x "")))
(assert (not (= x "AA")))
(assert (not (= x "<<")))
(assert (not (= x "99")))
(assert (not (= x "")))
(check-sat)
