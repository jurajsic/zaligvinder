(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re "FF----$$FF")) (str.to_re "++")))))
(assert (= (str.len x) 6))
(assert (not (= x "FF----$$FF++++")))
(assert (not (= x "++FF----$$FF++")))
(assert (not (= x "FF----$$FFFF----$$FF++")))
(check-sat)
