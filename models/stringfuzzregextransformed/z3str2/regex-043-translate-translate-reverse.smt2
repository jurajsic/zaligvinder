(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.union (str.to_re "U") (str.to_re "n")))))
(assert (= 3 (str.len x)))
(assert (not (= x "nnU")))
(assert (not (= x "UnU")))
(assert (not (= x "Unn")))
(assert (not (= x "nnn")))
(assert (not (= x "UUn")))
(assert (not (= x "nUn")))
(assert (not (= x "UUU")))
(check-sat)
