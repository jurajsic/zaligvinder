(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (str.to_re "}"))))
(assert (= (str.len x) 3))
(assert (not (= x "}}K")))
(assert (not (= x "K}K")))
(assert (not (= x "K}}")))
(assert (not (= x "}}}")))
(assert (not (= x "KK}")))
(assert (not (= x "}K}")))
(assert (not (= x "KKK")))
(check-sat)
