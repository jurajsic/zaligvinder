(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.union (str.to_re "bb") (re.+ (str.to_re ""))))))
(assert (str.in_re y (re.* (re.union (str.to_re "bb") (re.* (str.to_re ""))))))
(assert (not (= x y)))
(assert (= (str.len x) (str.to_int y)))
(check-sat)
