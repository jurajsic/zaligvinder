(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re "A")) (str.to_re "h")))))
(assert (str.in_re y (re.* (re.++ (re.* (str.to_re "A")) (str.to_re "h")))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 7))
(check-sat)
