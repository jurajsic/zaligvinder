(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.+ (str.to_re "C")) (str.to_re "")))))
(assert (str.in_re y (re.* (re.union (re.* (str.to_re "r")) (str.to_re "x")))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 11))
(check-sat)
