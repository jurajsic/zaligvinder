(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re "")) (str.to_re "bb")))))
(assert (str.in_re y (re.* (re.union (re.* (str.to_re "")) (str.to_re "mm")))))
(assert (not (= x y)))
(assert (= (str.len x) (str.to_int y)))
(check-sat)
