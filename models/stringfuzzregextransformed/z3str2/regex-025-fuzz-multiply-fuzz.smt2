(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.union (re.* (str.to_re "a'\t'")) (str.to_re "bb")))))
(assert (str.in_re y (re.* (re.union (re.+ (str.to_re "a")) (str.to_re "bZ")))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 0))
(check-sat)
