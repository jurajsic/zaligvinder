(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (= (str.++ y x) (str.++ n m)))
(assert (str.in_re n (re.* (str.to_re "/NcN'' ''`"))))
(assert (> (str.to_int x) (str.to_int m)))
(assert (= 1 (str.len n)))
(assert (= 0 (str.to_int y)))
(check-sat)
