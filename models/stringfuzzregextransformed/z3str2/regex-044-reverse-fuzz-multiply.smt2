(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.range "0" "9"))))
(assert (= 0 (str.to_int x)))
(assert (not (= x "11")))
(assert (not (= x "")))
(assert (not (= x "33")))
(assert (not (= x "%%")))
(assert (not (= x "((")))
(assert (not (= x "VV")))
(assert (not (= x "44")))
(assert (not (= x "TT")))
(assert (not (= x "77")))
(check-sat)
