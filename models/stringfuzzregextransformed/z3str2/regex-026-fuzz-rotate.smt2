(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.++ (re.+ (str.to_re "b")) (str.to_re "g")))))
(assert (str.in_re y (re.* (re.union (re.* (str.to_re "L")) (str.to_re "a")))))
(assert (not (= x y)))
(assert (= (str.to_int x) (str.to_int y)))
(check-sat)
