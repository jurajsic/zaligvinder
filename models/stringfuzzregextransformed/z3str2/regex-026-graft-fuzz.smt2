(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.union (re.* (str.to_re "")) (re.++ (re.* (str.to_re "m")) (str.to_re "+"))))))
(assert (str.in_re y (re.+ (str.to_re "b"))))
(assert (not (= x y)))
(assert (= (str.to_int x) (str.to_int y)))
(check-sat)
