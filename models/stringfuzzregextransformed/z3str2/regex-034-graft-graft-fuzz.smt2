(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (= (str.++ x y) (str.++ m n)))
(assert (str.in_re n (str.to_re "GBbY")))
(assert (> 1 (str.to_int m)))
(assert (= (str.to_int y) (str.len n)))
(assert (= (str.len x) 5))
(check-sat)
