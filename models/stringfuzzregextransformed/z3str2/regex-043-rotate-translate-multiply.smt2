(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.union (str.to_re "hh") (str.to_re "GG")))))
(assert (= 6 (str.len x)))
(assert (not (= x "hhGGGG")))
(assert (not (= x "hhGGhh")))
(assert (not (= x "GGGGhh")))
(assert (not (= x "GGGGGG")))
(assert (not (= x "GGhhhh")))
(assert (not (= x "GGhhGG")))
(assert (not (= x "hhhhhh")))
(check-sat)
