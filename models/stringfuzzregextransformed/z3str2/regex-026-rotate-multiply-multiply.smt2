(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re "bbbb")) (str.to_re "aaaa")))))
(assert (str.in_re y (re.* (re.++ (re.* (str.to_re "bbbb")) (str.to_re "aaaa")))))
(assert (not (= x y)))
(assert (= (str.len x) (str.len y)))
(check-sat)
