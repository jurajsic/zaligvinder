(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (= (str.++ x y) "Pb"))
(assert (str.in_re n (str.to_re (str.++ m n))))
(assert (> (str.len x) (str.len m)))
(assert (= 0 2))
(assert (= (str.to_int n) (str.to_int y)))
(check-sat)
