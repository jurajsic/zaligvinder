(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.union (re.+ (str.to_re "$")) (str.to_re "")))))
(assert (str.in_re y (re.+ (re.union (re.* (str.to_re "f")) (str.to_re "e")))))
(assert (= (str.len x) (str.to_int y)))
(assert (not (= x y)))
(assert (= (str.to_int x) 0))
(check-sat)
