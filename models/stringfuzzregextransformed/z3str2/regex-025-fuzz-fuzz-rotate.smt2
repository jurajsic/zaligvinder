(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.union (re.* (str.to_re "b")) (str.to_re "x")))))
(assert (str.in_re y (re.+ (re.++ (re.+ (str.to_re "{")) (str.to_re "a")))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 3))
(check-sat)
