(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re "aaaaaaaa")) (str.to_re "bbbbbbbb")))))
(assert (= (str.len x) 24))
(assert (not (= x "aaaaaaaaaaaaaaaabbbbbbbb")))
(assert (not (= x "aaaaaaaabbbbbbbbbbbbbbbb")))
(assert (not (= x "bbbbbbbbaaaaaaaabbbbbbbb")))
(check-sat)
