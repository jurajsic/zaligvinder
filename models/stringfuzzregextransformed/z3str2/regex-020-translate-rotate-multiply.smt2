(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re "zz")) (str.to_re "''\\\\\\\\tt''")))))
(assert (= (str.len x) 6))
(assert (not (= x "''\\\\\\\\tt''zzzz")))
(assert (not (= x "zz''\\\\\\\\tt''zz")))
