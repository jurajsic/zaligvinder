(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.union (re.* (re.+ (str.to_re "a"))) (str.to_re "b")))))
(assert (str.in_re y (re.+ (re.++ (str.to_re "x") (str.to_re "{")))))
(assert (= (str.len x) 3))
(assert (not (= x y)))
(assert (= (str.len x) (str.len y)))
(check-sat)
