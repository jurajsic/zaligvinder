(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.union (str.to_re "a") (re.+ (str.to_re "b"))))))
(assert (str.in_re y (re.+ (re.union (str.to_re "") (re.* (str.to_re "b"))))))
(assert (= (str.to_int x) (str.to_int y)))
(assert (not (= x y)))
(assert (= (str.to_int x) 1))
(check-sat)
