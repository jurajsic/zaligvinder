(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.union (str.to_re "'") (re.+ (str.to_re "\\"))))))
(assert (str.in_re y (re.* (re.union (str.to_re "M") (re.+ (str.to_re ""))))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.to_int x) 6))
(check-sat)
