(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.union (str.to_re "bb") (re.+ (str.to_re "NN"))))))
(assert (str.in_re y (re.* (re.union (str.to_re "VV") (re.+ (str.to_re ""))))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.to_int x) 12))
(check-sat)
