(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (= (str.++ x y) (str.++ m n)))
(assert (str.in_re n (re.* (str.to_re ".@EVz|Bm-"))))
(assert (> (str.len x) (str.to_int m)))
(assert (= 4 (str.len n)))
(assert (= 2 (str.to_int y)))
(check-sat)
