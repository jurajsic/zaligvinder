(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.union (re.+ (str.to_re "a")) (str.to_re "")))))
(assert (str.in_re y (re.+ (str.to_re ":aC\\"))))
(assert (= (str.to_int x) (str.to_int y)))
(assert (not (= x y)))
(assert (= 19 (str.to_int x)))
(check-sat)
