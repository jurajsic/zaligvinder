(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re ">")) (re.* (str.to_re ">"))))))
(assert (str.in_re y (re.* (re.++ (str.to_re "p") (str.to_re "p")))))
(assert (= 7 (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) (str.len x)))
(check-sat)
