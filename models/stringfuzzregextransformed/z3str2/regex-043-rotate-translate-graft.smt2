(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (str.to_re "h"))))
(assert (= (str.len x) 3))
(assert (not (= x "hGG")))
(assert (not (= x "hGh")))
(assert (not (= x "GGh")))
(assert (not (= x "GGG")))
(assert (not (= x "Ghh")))
(assert (not (= x "GhG")))
(assert (not (= x "hhh")))
(check-sat)
