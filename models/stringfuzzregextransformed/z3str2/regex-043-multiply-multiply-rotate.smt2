(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.union (str.to_re "aaaa") (str.to_re "bbbb")))))
(assert (= 12 (str.len x)))
(assert (not (= x "aaaabbbbbbbb")))
(assert (not (= x "aaaabbbbaaaa")))
(assert (not (= x "bbbbbbbbaaaa")))
(assert (not (= x "bbbbbbbbbbbb")))
(assert (not (= x "bbbbaaaaaaaa")))
(assert (not (= x "bbbbaaaabbbb")))
(assert (not (= x "aaaaaaaaaaaa")))
(check-sat)
