(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.union (re.+ (str.to_re "X")) (str.to_re "*"))))
(assert (str.in_re x (re.++ (re.union (re.* (str.to_re "B")) (re.+ (str.to_re ""))) (str.to_re ""))))
