(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.+ (str.to_re "oz/5")) (str.to_re "'\n'")))))
(assert (str.in_re y (re.* (re.union (re.* (str.to_re "")) (str.to_re "$")))))
(assert (= (str.len x) (str.to_int y)))
(assert (not (= x y)))
(assert (= (str.to_int x) 4))
(check-sat)
