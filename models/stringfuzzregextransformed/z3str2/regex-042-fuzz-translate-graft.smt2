(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.++ (str.to_re "") (re.+ (str.to_re "'\\r'|'\\r'")))))
(assert (str.in_re x (re.union (str.to_re "") (re.++ (re.+ (re.+ (str.to_re "e"))) (str.to_re "I")))))
(assert (= (str.to_int x) 0))
(check-sat)
