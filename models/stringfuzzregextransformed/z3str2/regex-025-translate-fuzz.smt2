(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.union (re.* (str.to_re "N")) (str.to_re "|")))))
(assert (str.in_re y (re.* (re.++ (re.* (str.to_re "M")) (str.to_re "|")))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.to_int x) 1))
(check-sat)
