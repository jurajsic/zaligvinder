(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.++ (re.+ (str.to_re "h")) (str.to_re "j")))))
(assert (str.in_re y (re.+ (re.++ (re.+ (str.to_re "h")) (str.to_re "H")))))
(assert (= (str.to_int x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 2))
(check-sat)
