(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re "####")) (str.to_re "yyyy")))))
(assert (str.in_re y (re.* (re.++ (re.* (str.to_re "####")) (str.to_re "yyyy")))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 28))
(check-sat)
