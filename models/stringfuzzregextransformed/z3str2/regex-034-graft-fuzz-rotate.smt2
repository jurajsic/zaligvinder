(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (= "Ebc" (str.++ m n)))
(assert (str.in_re n (str.to_re (str.++ x y))))
(assert (> (str.len x) (str.len m)))
(assert (= (str.to_int y) (str.len n)))
(assert (= 0 3))
(check-sat)
