(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re "'\u{b}'")) (str.to_re "k")))))
(assert (str.in_re y (re.* (str.to_re "'\u{b}'"))))
(assert (= 7 (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) (str.len x)))
(check-sat)
