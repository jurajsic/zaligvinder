(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.union (str.to_re "q") (str.to_re "|")))))
(assert (= 3 (str.len x)))
(assert (not (= x "q||")))
(assert (not (= x "q|q")))
(assert (not (= x "||q")))
(assert (not (= x "|||")))
(assert (not (= x "|qq")))
(assert (not (= x "|q|")))
(assert (not (= x "qqq")))
(check-sat)
