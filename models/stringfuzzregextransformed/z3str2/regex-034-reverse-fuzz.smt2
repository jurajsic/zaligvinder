(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (= (str.++ y x) (str.++ n m)))
(assert (str.in_re n (re.* (str.to_re "bk+b"))))
(assert (> (str.to_int x) (str.to_int m)))
(assert (= 4 (str.to_int n)))
(assert (= 2 (str.len y)))
(check-sat)
