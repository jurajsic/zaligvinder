(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (str.to_re "bb") (re.* (re.* (str.to_re "aa")))))))
(assert (str.in_re y (re.* (re.++ (str.to_re "bb") (str.to_re "aa")))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= 14 (str.len x)))
(check-sat)
