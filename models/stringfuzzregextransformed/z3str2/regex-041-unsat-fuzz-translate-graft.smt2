(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.union (str.to_re "^") (re.+ (str.to_re ".")))))
(assert (str.in_re x (re.union (str.to_re "A") (re.* (re.* (str.to_re "O"))))))
(assert (str.in_re x (re.++ (str.to_re "t") (re.++ (str.to_re "'\\r'") (re.+ (str.to_re "-"))))))
(check-sat)
