(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.union (str.to_re "w") (re.* (str.to_re "`")))))
(assert (str.in_re x (re.union (str.to_re "]") (re.* (str.to_re "a")))))
(assert (str.in_re x (re.union (str.to_re "}") (re.++ (re.+ (str.to_re "")) (str.to_re "a")))))
(check-sat)
