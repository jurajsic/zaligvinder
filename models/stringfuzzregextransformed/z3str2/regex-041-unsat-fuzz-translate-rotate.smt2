(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.union (str.to_re ".") (re.+ (str.to_re "^")))))
(assert (str.in_re x (re.union (str.to_re "'\\r'") (re.* (str.to_re "A")))))
(assert (str.in_re x (re.++ (re.* (re.+ (str.to_re "-"))) (re.++ (str.to_re "O") (str.to_re "t")))))
(check-sat)
