(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.++ (str.to_re "*") (re.+ (str.to_re "i")))))
(assert (str.in_re x (re.++ (str.to_re "") (re.union (re.+ (str.to_re "")) (str.to_re ",")))))
