(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (= (str.++ x y) (str.++ m n)))
(assert (str.in_re n (re.+ (str.to_re "aQ,6S"))))
(assert (> (str.len x) (str.len m)))
(assert (= 5 (str.len n)))
(assert (= 0 (str.to_int y)))
(check-sat)
