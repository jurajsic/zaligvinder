(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re "bbbb")) (str.to_re "aaaa")))))
(assert (= (str.len x) 12))
(assert (not (= x "aaaaaaaabbbb")))
(assert (not (= x "aaaabbbbbbbb")))
(assert (not (= x "bbbbaaaabbbb")))
(check-sat)
