(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.union (str.to_re "bb") (str.to_re "MM")))))
(assert (= 6 (str.len x)))
(assert (not (= x "MMMMbb")))
(assert (not (= x "bbMMbb")))
(assert (not (= x "bbMMMM")))
(assert (not (= x "MMMMMM")))
(assert (not (= x "bbbbMM")))
(assert (not (= x "MMbbMM")))
(assert (not (= x "bbbbbb")))
(check-sat)
