(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.++ (re.* (str.to_re "a")) (str.to_re "bb")))))
(assert (str.in_re y (re.* (re.++ (re.+ (str.to_re "aG")) (str.to_re "5yb")))))
(assert (= (str.len x) (str.to_int y)))
(assert (not (= x y)))
(assert (= (str.to_int x) 24))
(check-sat)
