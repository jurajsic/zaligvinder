(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.union (re.+ (str.to_re "a")) (str.to_re "b")))))
(assert (str.in_re y (re.+ (re.union (re.+ (str.to_re "-l")) (str.to_re "'c0x\\'")))))
(assert (not (= x y)))
(assert (= (str.len x) (str.len y)))
(check-sat)
