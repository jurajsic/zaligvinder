(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (str.to_re "{"))))
(assert (= (str.len x) 3))
(assert (not (= x "{ZZ")))
(assert (not (= x "{Z{")))
(assert (not (= x "ZZ{")))
(assert (not (= x "ZZZ")))
(assert (not (= x "Z{{")))
(assert (not (= x "Z{Z")))
(assert (not (= x "{{{")))
(check-sat)
