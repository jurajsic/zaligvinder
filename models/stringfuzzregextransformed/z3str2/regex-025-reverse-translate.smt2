(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (str.to_re "h") (re.* (str.to_re "A"))))))
(assert (str.in_re y (re.* (re.++ (str.to_re "h") (re.* (str.to_re "A"))))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 7))
(check-sat)
