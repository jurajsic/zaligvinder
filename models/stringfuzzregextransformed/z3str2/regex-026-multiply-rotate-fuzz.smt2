(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.+ (str.to_re """?'\r'")) (str.to_re "aa")))))
(assert (str.in_re y (re.* (re.union (re.+ (str.to_re "_NE")) (str.to_re "a*")))))
(assert (not (= x y)))
(assert (= (str.to_int x) (str.len y)))
(check-sat)
