(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re "yy")) (str.to_re "HHZZoo00AAHH")))))
(assert (str.in_re y (re.* (re.++ (re.* (str.to_re "yy")) (str.to_re "HHZZoo00AAHH")))))
(assert (not (= x y)))
(assert (= (str.len x) (str.len y)))
(check-sat)
