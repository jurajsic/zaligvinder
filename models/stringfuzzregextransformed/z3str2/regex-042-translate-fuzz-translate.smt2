(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.union (str.to_re "2") (re.* (str.to_re "Z")))))
(assert (str.in_re x (re.union (str.to_re "^") (re.++ (re.* (str.to_re "f")) (re.* (str.to_re ""))))))
(assert (= 0 (str.to_int x)))
(check-sat)
