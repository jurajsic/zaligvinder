(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.union (str.to_re "9") (re.+ (str.to_re "=")))))
(assert (str.in_re x (re.union (re.+ (re.* (str.to_re "'\r'"))) (re.++ (str.to_re "Z") (str.to_re ":")))))
(assert (= 1 (str.len x)))
(check-sat)
