(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (str.to_re "hh") (re.* (str.to_re "AA"))))))
(assert (str.in_re y (re.* (re.++ (str.to_re "hh") (re.* (str.to_re "AA"))))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 14))
(check-sat)
