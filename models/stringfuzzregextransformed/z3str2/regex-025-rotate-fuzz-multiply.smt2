(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.+ (str.to_re "bb")) (str.to_re "")))))
(assert (str.in_re y (re.* (re.union (re.* (str.to_re "XX")) (str.to_re "aa")))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 22))
(check-sat)
