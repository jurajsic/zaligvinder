(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (= (str.++ x y) (str.++ m n)))
(assert (str.in_re n (re.* (str.to_re "bY'"))))
(assert (> (str.to_int x) (str.len m)))
(assert (= 1 (str.to_int n)))
(assert (= 2 (str.len y)))
(check-sat)
