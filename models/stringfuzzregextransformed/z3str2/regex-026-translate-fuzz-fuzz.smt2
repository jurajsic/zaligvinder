(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.union (re.+ (str.to_re "Z")) (str.to_re "!")))))
(assert (str.in_re y (re.+ (re.union (re.+ (str.to_re "I")) (str.to_re "*")))))
(assert (not (= x y)))
(assert (= (str.to_int x) (str.len y)))
(check-sat)
