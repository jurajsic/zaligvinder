(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (str.to_re "bbbb") (re.* (str.to_re "aaaa"))))))
(assert (str.in_re y (re.* (re.++ (str.to_re "bbbb") (re.* (str.to_re "aaaa"))))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 28))
(check-sat)
