(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.union (str.to_re "a") (re.+ (str.to_re "'\u{c}'"))))))
(assert (str.in_re y (re.+ (re.++ (str.to_re "\\") (re.+ (str.to_re "b"))))))
(assert (not (= x y)))
(assert (= (str.to_int x) (str.to_int y)))
(check-sat)
