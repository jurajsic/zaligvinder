(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.union (re.+ (str.to_re "K")) (str.to_re "")))))
(assert (str.in_re y (re.+ (re.union (re.* (str.to_re "2")) (str.to_re "a")))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 4))
(check-sat)
