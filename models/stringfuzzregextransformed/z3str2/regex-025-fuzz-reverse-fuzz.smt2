(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re "a")) (str.to_re "b")))))
(assert (str.in_re y (re.+ (re.++ (re.+ (str.to_re "a")) (str.to_re "]")))))
(assert (= (str.len x) (str.to_int y)))
(assert (not (= x y)))
(assert (= (str.len x) 3))
(check-sat)
