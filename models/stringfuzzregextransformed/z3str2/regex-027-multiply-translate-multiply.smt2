(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re "CCCC")) (str.to_re "YYYY")))))
(assert (= (str.len x) 12))
(assert (not (= x "CCCCCCCCYYYY")))
(assert (not (= x "CCCCYYYYYYYY")))
(assert (not (= x "YYYYCCCCYYYY")))
(check-sat)
