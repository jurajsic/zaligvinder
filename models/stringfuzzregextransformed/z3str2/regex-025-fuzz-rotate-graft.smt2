(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (str.to_re "a"))))
(assert (str.in_re y (re.* (re.union (re.+ (str.to_re "b")) (re.union (re.* (str.to_re "b")) (str.to_re "a"))))))
(assert (= (str.len x) 2))
(assert (not (= x y)))
(assert (= (str.len x) (str.to_int y)))
(check-sat)
