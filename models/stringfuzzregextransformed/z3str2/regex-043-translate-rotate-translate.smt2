(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.union (str.to_re "E") (str.to_re "H")))))
(assert (= 3 (str.len x)))
(assert (not (= x "EHH")))
(assert (not (= x "EHE")))
(assert (not (= x "HHE")))
(assert (not (= x "HHH")))
(assert (not (= x "HEE")))
(assert (not (= x "HEH")))
(assert (not (= x "EEE")))
(check-sat)
