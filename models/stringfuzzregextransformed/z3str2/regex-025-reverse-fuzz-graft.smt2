(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (str.to_re "V")))
(assert (str.in_re y (re.* (re.union (re.* (re.union (str.to_re "b") (re.+ (str.to_re "N")))) (re.+ (str.to_re ""))))))
(assert (= (str.len x) 6))
(assert (not (= x y)))
(assert (= (str.to_int x) (str.len y)))
(check-sat)
