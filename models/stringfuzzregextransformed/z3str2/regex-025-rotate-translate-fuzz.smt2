(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.union (re.* (str.to_re "}")) (str.to_re "p")))))
(assert (str.in_re y (re.* (re.union (re.+ (str.to_re "Z")) (str.to_re "p")))))
(assert (= (str.len x) (str.to_int y)))
(assert (not (= x y)))
(assert (= (str.to_int x) 8))
(check-sat)
