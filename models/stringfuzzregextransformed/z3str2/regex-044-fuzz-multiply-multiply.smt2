(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.range "0" "9"))))
(assert (= 4 (str.to_int x)))
(assert (not (= x "1111")))
(assert (not (= x "0000")))
(assert (not (= x "3333")))
(assert (not (= x "2222")))
(assert (not (= x "")))
(assert (not (= x "AAAA")))
(assert (not (= x "<<<<")))
(assert (not (= x "9999")))
(assert (not (= x "")))
(check-sat)
