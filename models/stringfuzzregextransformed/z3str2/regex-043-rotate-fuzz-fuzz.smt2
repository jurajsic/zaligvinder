(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.+ (re.union (str.to_re "") (str.to_re "")))))
(assert (= 0 (str.to_int x)))
(assert (not (= x "?b")))
(assert (not (= x "")))
(assert (not (= x "x")))
(assert (not (= x "b\\")))
(assert (not (= x "b7#XPa3")))
(assert (not (= x "gql")))
(assert (not (= x "M")))
(check-sat)
