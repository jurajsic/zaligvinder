(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.++ (re.+ (str.to_re "b)H")) (str.to_re "a")))))
(assert (str.in_re y (re.+ (re.union (re.* (str.to_re "jx")) (str.to_re "")))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 8))
(check-sat)
