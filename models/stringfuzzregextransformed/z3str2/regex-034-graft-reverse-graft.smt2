(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (= (str.++ y x) (str.++ n m)))
(assert (str.in_re n (str.to_re "cba")))
(assert (> (str.len x) 3))
(assert (= (str.len y) (str.len n)))
(assert (= 1 (str.len m)))
(check-sat)
