(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.++ (re.+ (str.to_re "GG")) (str.to_re "")))))
(assert (= (str.len x) 2))
(assert (not (= x "aaaaaa..2288dd]]mm")))
(assert (not (= x "aaVVII||OO]]II")))
(assert (not (= x "bb""""""""''\\\\rr''[[..%%bbbb")))
(check-sat)
