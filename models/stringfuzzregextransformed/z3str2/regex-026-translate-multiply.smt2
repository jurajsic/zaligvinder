(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re "cc")) (str.to_re "''\\\\u{x0}0bb''")))))
(assert (str.in_re y (re.* (re.++ (re.* (str.to_re "cc")) (str.to_re "''\\\\u{x0}0bb''")))))
(assert (not (= x y)))
(assert (= (str.len x) (str.len y)))
(check-sat)
