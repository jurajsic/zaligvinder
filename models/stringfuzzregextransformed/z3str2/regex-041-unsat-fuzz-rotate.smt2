(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.union (str.to_re "b") (re.+ (str.to_re "a")))))
(assert (str.in_re x (re.union (str.to_re "S") (re.* (str.to_re "K")))))
(assert (str.in_re x (re.++ (re.* (re.+ (str.to_re ":"))) (re.++ (str.to_re "r") (str.to_re "X")))))
(check-sat)
