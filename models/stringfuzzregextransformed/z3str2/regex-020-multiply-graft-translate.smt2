(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (str.to_re "'\r''\r'") (re.* (str.to_re "GG"))))))
(assert (= 6 (str.len x)))
(assert (not (= x "GG'\r''\r''\r''\r'")))
(assert (not (= x "'\r''\r'GG'\r''\r'")))
(assert (not (= x "GGGG'\r''\r'")))
(check-sat)
