(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.union (re.+ (str.to_re "q3")) (str.to_re "b")))))
(assert (str.in_re y (re.+ (re.union (re.* (str.to_re "ae")) (str.to_re "`")))))
(assert (not (= x y)))
(assert (= (str.len x) (str.to_int y)))
(check-sat)
