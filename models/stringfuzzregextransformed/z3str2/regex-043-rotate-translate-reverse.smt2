(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.union (str.to_re "h") (str.to_re "G")))))
(assert (= 3 (str.len x)))
(assert (not (= x "GGh")))
(assert (not (= x "hGh")))
(assert (not (= x "hGG")))
(assert (not (= x "GGG")))
(assert (not (= x "hhG")))
(assert (not (= x "GhG")))
(assert (not (= x "hhh")))
(check-sat)
