(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.++ (re.union (re.+ (str.to_re "n")) (re.+ (str.to_re "z"))) (re.* (str.to_re "{")))))
(assert (str.in_re x (re.union (str.to_re "c") (str.to_re "<"))))
(assert (= (str.to_int x) 5))
(check-sat)
