(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.union (re.* (str.to_re "")) (str.to_re "")))))
(assert (str.in_re y (re.* (re.++ (re.* (str.to_re "1")) (str.to_re "%")))))
(assert (= (str.to_int x) (str.to_int y)))
(assert (not (= x y)))
(assert (= (str.to_int x) 10))
(check-sat)
