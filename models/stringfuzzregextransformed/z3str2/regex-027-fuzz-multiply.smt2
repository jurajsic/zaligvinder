(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re "aa")) (str.to_re "")))))
(assert (= (str.len x) 0))
(assert (not (= x "aaaa11PP""""""""")))
(assert (not (= x "''' '' '''''' '' '''33CCtt")))
(assert (not (= x "aaHH;;")))
(check-sat)
