(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.+ (str.to_re "")) (str.to_re "b")))))
(assert (str.in_re y (re.* (re.union (re.* (str.to_re "a")) (str.to_re "X")))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 11))
(check-sat)
