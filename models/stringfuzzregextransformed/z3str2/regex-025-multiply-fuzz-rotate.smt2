(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.++ (re.* (str.to_re "UTb")) (str.to_re "P")))))
(assert (str.in_re y (re.+ (re.union (re.+ (str.to_re "")) (str.to_re "aa")))))
(assert (= (str.len x) (str.to_int y)))
(assert (not (= x y)))
(assert (= (str.len x) 6))
(check-sat)
