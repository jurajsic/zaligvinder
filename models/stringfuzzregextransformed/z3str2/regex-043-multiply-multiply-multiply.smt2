(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.union (str.to_re "aaaaaaaa") (str.to_re "bbbbbbbb")))))
(assert (= 24 (str.len x)))
(assert (not (= x "aaaaaaaabbbbbbbbbbbbbbbb")))
(assert (not (= x "aaaaaaaabbbbbbbbaaaaaaaa")))
(assert (not (= x "bbbbbbbbbbbbbbbbaaaaaaaa")))
(assert (not (= x "bbbbbbbbbbbbbbbbbbbbbbbb")))
(assert (not (= x "bbbbbbbbaaaaaaaaaaaaaaaa")))
(assert (not (= x "bbbbbbbbaaaaaaaabbbbbbbb")))
(assert (not (= x "aaaaaaaaaaaaaaaaaaaaaaaa")))
(check-sat)
