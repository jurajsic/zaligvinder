(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.union (re.+ (re.* (str.to_re "a"))) (str.to_re "")))))
(assert (str.in_re y (re.+ (re.union (str.to_re "") (str.to_re "4")))))
(assert (not (= x y)))
(assert (= (str.to_int x) (str.to_int y)))
(check-sat)
