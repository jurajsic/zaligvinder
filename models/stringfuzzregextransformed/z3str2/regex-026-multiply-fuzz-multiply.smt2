(set-logic QF_SLIA)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.union (re.+ (str.to_re "aa")) (str.to_re "bb")))))
(assert (str.in_re y (re.+ (re.union (re.+ (str.to_re "ll--")) (str.to_re "''\\\\u{x0}0cc''")))))
(assert (not (= x y)))
(assert (= (str.len x) (str.len y)))
(check-sat)
